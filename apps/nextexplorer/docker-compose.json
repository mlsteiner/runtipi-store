{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "nextexplorer",
      "image": "nxzai/explorer:latest",
      "isMain": true,
      "internalPort": 3010
    }
  ],
  "volumes": [
    {
      "hostPath": "${APP_DATA_DIR}/nextexplorer/config",
      "containerPath": "/config",
      "readOnly": false,
      "shared": false,
      "private": false
    }
  ]
}
